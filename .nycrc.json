{
  "extends": "@istanbuljs/nyc-config-typescript",
  "all": true,
  "include": ["src/**/*.ts", "src/**/*.svelte"],
  "exclude": [
    "**/*.test.ts",
    "**/*.spec.ts",
    "src/app.d.ts",
    "node_modules/**",
    ".svelte-kit/**"
  ],
  "reporter": ["text", "html", "lcov"],
  "report-dir": "coverage-e2e",
  "temp-dir": ".nyc_output",
  "check-coverage": false
}
